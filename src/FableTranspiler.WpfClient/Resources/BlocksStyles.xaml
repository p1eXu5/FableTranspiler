<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
                    >


    <Style x:Key="st_Keyword" TargetType="Run">
        <Setter Property="Foreground" Value="#3987D6"/>
    </Style>

    <Style x:Key="st_Type" TargetType="Run">
        <Setter Property="Foreground" Value="#4EC9B0"/>
    </Style>

    <Style x:Key="st_Comment" TargetType="Run">
        <Setter Property="Foreground" Value="#39904A"/>
    </Style>

    <Style x:Key="st_Modifier" TargetType="Run">
        <Setter Property="Foreground" Value="#888888"/>
    </Style>

</ResourceDictionary>